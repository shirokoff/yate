/// {
///     description: 'existence of nodeset',
///     data: {
///         a: 'foo',
///         b: 42,
///         c: true,
///         d: false,
///         e: null,
///         f: 0,
///         g: '',
///         h: [],
///         i: {},
///
///         j: [ 1, 2, 3 ],
///         k: {
///             foo: true
///         }
///     },
///     result: 'abcdefgijk'
/// }

match / {

    if exists(.a) {
        "a"
    }
    if exists(.b) {
        "b"
    }
    if exists(.c) {
        "c"
    }
    if exists(.d) {
        "d"
    }
    if exists(.e) {
        "e"
    }
    if exists(.f) {
        "f"
    }
    if exists(.g) {
        "g"
    }
    if exists(.h) {
        "h"
    }
    if exists(.i) {
        "i"
    }
    if exists(.j) {
        "j"
    }
    if exists(.k) {
        "k"
    }
    if exists(.l) {
        "l"
    }

}

