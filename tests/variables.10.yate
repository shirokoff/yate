/// {
///     description: 'global non-constant variable',
///     data: {
///         item: [
///             { title: 'First' },
///             { title: 'Second' },
///             { title: 'Third' }
///         ]
///     },
///     result: 'FirstSecondThird'
/// }

items = .item

match / {
    apply items
}

match .item {
    .title
}

