/// {
///     description: 'nested arrays',
///     data: [
///         [ { foo: 1 }, { foo: 2 }, { foo: 3 } ],
///         [ { foo: 4 }, { foo: 5 }, { foo: 6 } ],
///         [ { foo: 7 }, { foo: 8 }, { foo: 9 } ]
///     ],
///     result: '<table><tr><td>1</td><td>2</td><td>3</td></tr><tr><td>4</td><td>5</td><td>6</td></tr><tr><td>7</td><td>8</td><td>9</td></tr></table>'
/// }

match / {
    <table>
        apply .item row
    </table>
}

match .item row {
    <tr>
        apply .item cell
    </tr>
}

match .item cell {
    <td>
        .foo
    </td>
}

