/// {
///     description: 'falsy values',
///     data: {
///         a: 'foo',
///         b: 42,
///         c: true,
///         d: false,
///         e: null,
///         f: 0,
///         g: '',
///         h: [],
///         i: {},
///
///         j: [ 1, 2, 3 ],
///         k: {
///             foo: true
///         }
///     },
///     result: 'abci!d!e!f!g!h'
/// }

match / {

    if .a {
        "a"
    }
    if .b {
        "b"
    }
    if .c {
        "c"
    }
    if .d {
        "d"
    }
    if .e {
        "e"
    }
    if .f {
        "f"
    }
    if .g {
        "g"
    }
    if .h {
        "h"
    }
    if .i {
        "i"
    }

    if !.a {
        "!a"
    }
    if !.b {
        "!b"
    }
    if !.c {
        "!c"
    }
    if !.d {
        "!d"
    }
    if !.e {
        "!e"
    }
    if !.f {
        "!f"
    }
    if !.g {
        "!g"
    }
    if !.h {
        "!h"
    }
    if !.i {
        "!i"
    }

}

