/// {
///     description: 'function returning boolean',
///     data: {
///         valid: true,
///         count: 42
///     },
///     result: '<div>Ok</div><div>Valid</div>'
/// }

func ok() {
    .count > 24 && .count % 2 == 0
}

func valid() {
    !!.valid
}

match / {
    if ok() {
        <div>Ok</div>
    }

    if valid() {
        <div>Valid</div>
    }
}

