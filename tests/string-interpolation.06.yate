/// {
///     description: 'multiple interpolations inside inline attribute',
///     data: {
///         id: 33662468,
///         username: 'nop'
///     },
///     result: '<h1 class="b-hello b-hello-nop b-hello-33662468">Hello, nop</h1>'
/// }

match / {
    <h1 class="b-hello b-hello-{ .username } b-hello-{ .id }">
        "Hello, { .username }"
    </h1>
}

